{
    "name": "textlint-plugin-org",
    "version": "0.1.0",
    "description": "Textlint plugin for Org mode",
    "repository": {
        "type": "git",
        "url": "git@github.com:kijimaD/textlint-plugin-org.git"
    },
    "author": "Kijima Daigo",
    "license": "GPLv3",
    "bugs": {
        "url": "https://github.com/kijimaD/textlint-plugin-org/issues"
    },
    "homepage": "https://github.com/kijimaD/textlint-plugin-org#readme",
    "main": "lib/index.js",
    "files": [
        "lib",
        "src"
    ],
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
        "watch": "babel src --out-dir lib --watch --source-maps",
        "prepublish": "npm run --if-present build",
        "test": "mocha --require @babel/register test/*test.js",
        "lint": "npx eslint src/*.js test/*.js"
    },
    "dependencies": {
        "@babel/cli": "^7.14.3",
        "@babel/core": "^7.14.3",
        "@babel/eslint-plugin": "^7.13.16",
        "@babel/preset-env": "^7.14.4",
        "@babel/register": "^7.13.16",
        "@orgajs/reorg": "^2.4.8",
        "@orgajs/reorg-rehype": "^2.4.8",
        "@types/date-fns": "^2.6.0",
        "babel-eslint": "^10.1.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.26.0",
        "date-fns": "^2.22.1",
        "eslint-config-airbnb-base": "^14.2.1",
        "npm-check-updates": "^11.6.0",
        "orga": "^2.4.8",
        "rehype-stringify": "^8.0.0",
        "reorg-parse": "^2.3.16",
        "structured-source": "^3.0.2",
        "traverse": "^0.6.6",
        "unified": "^9.2.1",
        "unified-stream": "^1.0.6"
    },
    "devDependencies": {
        "@textlint/ast-tester": "^12.0.0",
        "babel-preset-jsdoc-to-assert": "^5.0.0",
        "babel-preset-power-assert": "^3.0.0",
        "eslint": "^7.28.0",
        "eslint-plugin-import": "^2.23.4",
        "jsdoc-to-assert": "^2.8.0",
        "mocha": "^8.4.0",
        "power-assert": "^1.6.1",
        "textlint": "^12.0.0",
        "textlint-plugin-html": "^0.2.0",
        "textlint-rule-max-comma": "^2.0.2",
        "textlint-rule-no-todo": "^2.0.1"
    }
}
